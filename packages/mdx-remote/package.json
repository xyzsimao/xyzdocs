  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "gray-matter": "^4.0.3",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/mdx": "^2.0.13",
    "@types/node": "25.1.0",
    "@types/react": "^19.2.10",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "xyzdocs-core": "workspace:*",
    "react": "^19.2.4",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.19.0"
  },
  "peerDependencies": {
    "@types/react": "*",
    "xyzdocs-core": "^15.0.0 || ^16.0.0",
    "react": "18.x.x || 19.x.x"
  },
  "peerDependenciesMeta": {
